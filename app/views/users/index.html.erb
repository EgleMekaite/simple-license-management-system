<h1>Users for <%= @account.name %></h1>

<p>
  <%= link_to "New User", new_account_user_path(@account) %>
  |
  <%= link_to "Back to Account", account_path(@account) %>
</p>

<% if @users.any? %>
  <ul>
    <% @users.each do |user| %>
      <li><%= user.name %> (<%= user.email %>)</li>
    <% end %>
  </ul>
<% else %>
  <p>No users yet.</p>
<% end %>



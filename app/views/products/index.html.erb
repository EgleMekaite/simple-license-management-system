<h1>Products</h1>

<p>
  <%= link_to "New Product", new_product_path %>
</p>

<% if @products.any? %>
  <ul>
    <% @products.each do |product| %>
      <li>
        <%= link_to product.name, product_path(product) %>
      </li>
    <% end %>
  </ul>
<% else %>
  <p>No products yet.</p>
<% end %>



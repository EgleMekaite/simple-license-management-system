<h1>Accounts</h1>

<p>
  <%= link_to "New Account", new_account_path %>
</p>

<% if @accounts.any? %>
  <ul>
    <% @accounts.each do |account| %>
      <li>
        <%= link_to account.name, account_path(account) %>
      </li>
    <% end %>
  </ul>
<% else %>
  <p>No accounts yet.</p>
<% end %>


